<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans default-autowire="byName">

    <bean id="flashAuthListener" class="net.ds3783.chatserver.extension.core.FlashAuthListener"
          init-method="init"/>
    <bean id="loginListener" class="net.ds3783.chatserver.extension.core.LoginListener" init-method="init"/>
    <bean id="blackListListener" class="net.ds3783.chatserver.extension.core.BlackListListener"
          init-method="init"/>


    <bean id="listChannelListener" class="net.ds3783.chatserver.extension.command.ListChannelListener"
          init-method="init"/>
    <bean id="joinChannelListener" class="net.ds3783.chatserver.extension.command.JoinChannelListener"
          init-method="init"/>
    <bean id="exitChannelListener" class="net.ds3783.chatserver.extension.command.ExitChannelListener"
          init-method="init"/>
    <bean id="createChannelListener" class="net.ds3783.chatserver.extension.command.CreateChannelListener"
          init-method="init"/>


    <bean id="updateClientListListener" class="net.ds3783.chatserver.extension.command.UpdateClientListListener"
          init-method="init"/>


    <bean id="publicMessageListener" class="net.ds3783.chatserver.extension.chat.PublicMessageListener"
          init-method="init"/>
    <bean id="privateMessageListener" class="net.ds3783.chatserver.extension.chat.PrivateMessageListener"
          init-method="init"/>


</beans>